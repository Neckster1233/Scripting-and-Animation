 //Create Base Cube
polyCube -width 5 -height 5 -depth 5 -subdivisionsX 1 -subdivisionsY 1 -subdivisionsZ 1 -axis 0 1 0 -createUVs 4 -constructionHistory 1; 
move -relative 0 2.5 0 ;

//Create Roof
polyCone -radius 1 -height 2 -subdivisionsX 20 -subdivisionsY 1 -subdivisionsZ 0 -axis 0 1 0 -roundCap 0 -createUVs 3 -constructionHistory 1;
move -relative 0 6 0 ;
scale -relative 3.85 1 3.85 ;

//Create Left Doorframe
polyCube -width 1 -height 1 -depth 1 -subdivisionsX 1 -subdivisionsY 1 -subdivisionsZ 1 -axis 0 1 0 -createUVs 4 -constructionHistory 1; 
move -relative -.5 1.17 2.3 ;
scale -relative .17 2.3 1 ;

//Create Right Doorframe
polyCube -width 1 -height 1 -depth 1 -subdivisionsX 1 -subdivisionsY 1 -subdivisionsZ 1 -axis 0 1 0 -createUVs 4 -constructionHistory 1; 
move -relative .5 1.17 2.3 ;
scale -relative .17 2.3 1 ;

//Create Top Doorframe
polyCube -width 1 -height 1 -depth 1 -subdivisionsX 1 -subdivisionsY 1 -subdivisionsZ 1 -axis 0 1 0 -createUVs 4 -constructionHistory 1; 
move -relative 0 2.4 2.3 ;
scale -relative .17 2.3 1 ;
rotate -relative -objectSpace -forceOrderXYZ 0 0 90 ;

//Create Doorknob
polySphere -radius 1 -subdivisionsX 20 -subdivisionsY 20 -axis 0 1 0 -createUVs 2 -constructionHistory 1;
move -relative .285 1.23 2.675 ;
scale -relative .066 .066 .066 ;

//Create Left Window Frame
polyCube -width 1 -height 1 -depth 1 -subdivisionsX 1 -subdivisionsY 1 -subdivisionsZ 1 -axis 0 1 0 -createUVs 4 -constructionHistory 1; 
move -relative .946 1.42 2.1 ;
scale -relative .14 1 1 ;

//Create Right Window Frame
polyCube -width 1 -height 1 -depth 1 -subdivisionsX 1 -subdivisionsY 1 -subdivisionsZ 1 -axis 0 1 0 -createUVs 4 -constructionHistory 1; 
move -relative 1.8 1.42 2.1 ;
scale -relative .14 1 1 ;

//Create Top Window Frame
polyCube -width 1 -height 1 -depth 1 -subdivisionsX 1 -subdivisionsY 1 -subdivisionsZ 1 -axis 0 1 0 -createUVs 4 -constructionHistory 1; 
move -relative 1.37 2 2.1 ;
scale -relative .14 1 1 ;
rotate -relative -objectSpace -forceOrderXYZ 0 0 90 ;

//Create Bottom Window Frame
polyCube -width 1 -height 1 -depth 1 -subdivisionsX 1 -subdivisionsY 1 -subdivisionsZ 1 -axis 0 1 0 -createUVs 4 -constructionHistory 1; 
move -relative 1.37 .85 2.1 ;
scale -relative .14 1 1 ;
rotate -relative -objectSpace -forceOrderXYZ 0 0 90 ;

//Create Vertical Window X
polyCube -width 1 -height 1 -depth 1 -subdivisionsX 1 -subdivisionsY 1 -subdivisionsZ 1 -axis 0 1 0 -createUVs 4 -constructionHistory 1; 
move -relative 1.35 1.434 2.1 ;
scale -relative .123 1.05 .877 ;

//Create Horizontal Window X
polyCube -width 1 -height 1 -depth 1 -subdivisionsX 1 -subdivisionsY 1 -subdivisionsZ 1 -axis 0 1 0 -createUVs 4 -constructionHistory 1; 
move -relative 1.35 1.434 2.1 ;
scale -relative .123 .877 .877 ;
rotate -relative -objectSpace -forceOrderXYZ 0 0 90 ;